<!-- Overview tab -->
<div class="row">
  <!-- Usage column -->
  <div class="col-md-6 col-lg-4">
    <%= partial(:'filesystems/usage', locals: {fs: pool}) %>
  </div>

  <!-- Properties column -->
  <div class="properties col-md-6 col-lg-4">
